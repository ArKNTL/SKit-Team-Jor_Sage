<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/button2"
        android:layout_width="257dp"
        android:layout_height="45dp"
        android:background="@drawable/rounded_corner"
        android:text="Log In"
        android:textcolor="@color/filtersbuttoncolors"
        app:backgroundTint="#BFF7FF "
        tools:layout_editor_absoluteX="77dp"
        tools:layout_editor_absoluteY="580dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="Username/Email"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="383dp"
        android:gravity="center_horizontal" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="451dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="525dp"
        android:layout_height="324dp"
        app:srcCompat="@drawable/background_profile"
        tools:layout_editor_absoluteX="-34dp"
        tools:layout_editor_absoluteY="-23dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="162dp"
        android:layout_height="148dp"
        app:srcCompat="@drawable/profile"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="88dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Need account?Sign Up"
        android:textSize="20dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="506dp" />

</androidx.constraintlayout.widget.ConstraintLayout>